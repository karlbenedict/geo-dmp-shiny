      "item_name":"trust-r",
      "item_label":"TRUST R",
      "item_description":"To be responsible for ensuring the authenticity and integrity of data holdings and for the reliability and persistence of its service"
    },
    {
      "item_name":"trust-U",
      "item_label":"TRUST U",
      "item_description":"To ensure that the data management norms and expectations of target user communities are met"
    },
    {
      "item_name":"trust-s",
      "item_label":"TRUST S",
      "item_description":"To sustain services and preserve data holdings for the long-term"
    },
